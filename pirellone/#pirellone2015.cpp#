		for(int j=0; j<N; j++){
			in >> matrix[i][j];
		}
	}
	
	
	for(int j=0 ; j<N ; j++) {
	  //se l'elemento e' 1 inverto la colonna
	  if( matrix[0][j]==1){
	    for(int t=0; t<N; t++)
	      {
		if matrix[i][t]==0
	      }
	  }
	    
	}
	
	
